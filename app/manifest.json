{
  "name": "Zoom参加者一覧取得",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["tabs"],
  "browser_action": {
    "default_title": "Zoom参加者一覧取得",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://zoom.us/wc/*/join*"],
      "js": ["contentscript.js"]
    }
  ]
}
